        <mxCell id="U0TjRVrRfVp29wMPK2Ei-100" value="&lt;p style=&quot;margin:0px;margin-top:4px;text-align:center;&quot;&gt;&lt;b&gt;user  /* 用户表 */&lt;/b&gt;&lt;/p&gt;&lt;hr size=&quot;1&quot;/&gt;&lt;p style=&quot;margin:0 0 0 4px;line-height:1.6;&quot;&gt; password  /* 密码 */: varchar(64)&lt;br/&gt; userhash  /* 用户hash */: varchar(64)&lt;br/&gt; session  /* 登陆session */: varchar(100)&lt;br/&gt; loginTime  /* 登陆时间 */: int(10) unsigned&lt;br/&gt; addTime  /* 注册时间 */: int(10) unsigned&lt;br/&gt; isSub  /* 是否子成员；0=主账户；1=子成员 */: int(10) unsigned&lt;br/&gt; isOut  /* 是否出院 */: int(10) unsigned&lt;br/&gt; hExpireTime  /* 康复系统账号1个月需短信验证一次 */: int(10) unsigned&lt;br/&gt; medicalHistory  /* 病史json存储格式[{&amp;quot;date&amp;quot;:2020-07-30,&amp;quot;title&amp;quot;:&amp;quot;急性加重&amp;quot;,&amp;quot;detail&amp;quot;:&amp;quot;脸色发紫&amp;quot;}] */: text&lt;br/&gt; config  /* 用户配置信息json */: text&lt;br/&gt; parent  /* 父账户 */: varchar(45)&lt;br/&gt; name  /* 真实姓名 */: varchar(45)&lt;br/&gt; nickName  /* 姓名昵称 */: varchar(45)&lt;br/&gt; phone  /* 电话 */: varchar(16)&lt;br/&gt; email  /* 邮件 */: varchar(45)&lt;br/&gt; remarks  /* 备注说明 */: varchar(45)&lt;br/&gt; occupation  /* 职业 */: varchar(50)&lt;br/&gt; smokingHistory  /* 吸烟史/年 */: varchar(50)&lt;br/&gt; sex  /* 性别；0未知，1男2女 */: int(10) unsigned&lt;br/&gt; birthDay  /* 生日 */: varchar(10)&lt;br/&gt; city  /* 城市地区 */: varchar(100)&lt;br/&gt; thumb  /* 头像 */: varchar(100)&lt;br/&gt; address  /* 住址 */: varchar(100)&lt;br/&gt; race  /* 区域；0未知；1亚洲 */: int(10) unsigned&lt;br/&gt; stature  /* 身高 */: int(10) unsigned&lt;br/&gt; pneumoconiosisType  /* 尘肺病类型： 0.未选 1.矽肺 2.煤工尘肺 3.石墨尘肺 4.滑石尘肺 5.水泥尘肺 6.云母尘肺 7.陶工尘肺 8.... */: tinyint(4)&lt;br/&gt; occupationCode  /* 职业代码：0-其他 1-矿山开采；2-金属冶炼；3-机械制造；4-建筑材料；5-路桥施工；6-水利建设  */: tinyint(4)&lt;br/&gt; diseaseType  /* 病种类别 0无,1慢阻肺,2哮喘,3肺纤维化,4肺动脉高压,5尘肺I期,6尘肺II期,7尘肺III期 */: tinyint(4)&lt;br/&gt; nation  /* 民族 */: varchar(50)&lt;br/&gt; marital  /* 婚姻状况 */: varchar(50)&lt;br/&gt; symptom  /* 症状 */: varchar(100)&lt